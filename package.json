{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js >> log 2>&1 &",
    "test": "mocha",
    "heroku:pre": "git checkout heroku; git pull origin master;",
    "heroku:push": "git add . ; git commit -m 'new' ; git push heroku heroku:master; git checkout master",
    "heroku": "npm run heroku:pre && npm run heroku:push",
    "trans": "node translate.js all",
    "db": "mongod --dbpath ~/mongodb/beepmetro --quiet --logpath ~/mongodb/beepmetro/log &"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.0.2",
    "mongoose-unique-validator": "^2.0.0"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11"
  }
}
